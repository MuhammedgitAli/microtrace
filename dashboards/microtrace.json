{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "11.4.0"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": "11.4.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": "11.4.0"
    },
    {
      "type": "panel",
      "id": "bargauge",
      "name": "Bar gauge",
      "version": "11.4.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "2.53.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "name": "Annotations & Alerts",
        "type": "dashboard",
        "enable": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "datasource": {
          "type": "datasource",
          "uid": "grafana"
        }
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "timeseries",
      "title": "Request Throughput",
      "id": 1,
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(requests_total[1m])) by (method, status)",
          "legendFormat": "{{method}} {{status}}",
          "instant": false
        }
      ],
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (s)",
      "id": 2,
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(request_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "instant": false
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Requests by Path",
      "id": 3,
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(requests_total[5m])) by (path)",
          "legendFormat": "{{path}}",
          "instant": false
        }
      ]
    },
    {
      "type": "stat",
      "title": "Error Rate (5m)",
      "id": 4,
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(errors_total[5m]))",
          "instant": true
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["microtrace"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "title": "MicroTrace Overview",
  "uid": "microtrace-overview",
  "version": 1,
  "weekStart": ""
}
